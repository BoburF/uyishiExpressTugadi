<div class="container">
  <h1>Shaftoli.uz ga Xush kelibsiz</h1>

  <div class="row">
    {{#each card}}
      <div class="col s12 m3">
        <div class="card">
          <div class="card-image">
            <img src={{img}} />
            <span class="card-title">{{name}}</span>
          </div>
          <div class="card-content">
            <p>{{price}}</p>
          </div>
          <div class="card-action">
            <form action="/products/add" method="post">
                <input type="hidden" name="id" value="{{id}}" >
                <button type="submit">Избранные</button>
            </form>
            <a href="#">корзине</a>
          </div>
        </div>
      </div>
    {{/each}}
  </div>

</div>